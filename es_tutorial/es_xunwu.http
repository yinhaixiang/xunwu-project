###  创建一个新的xunwu索引
PUT http://localhost:9200/xunwu
Content-Type: application/json

{
  "settings": {
    "number_of_shards": 5,
    "number_of_replicas": 0
  },
  "mappings": {
    "dynamic": false,
    "properties": {
      "houseId": {
        "type": "long"
      },
      "title": {
        "type": "text"
      },
      "price": {
        "type": "integer"
      },
      "area": {
        "type": "integer"
      },
      "createTime": {
        "type": "date"
      },
      "lastUpdateTime": {
        "type": "date"
      },
      "cityEnName": {
        "type": "keyword"
      },
      "regionEnName": {
        "type": "keyword"
      },
      "direction": {
        "type": "integer"
      },
      "distanceToSubway": {
        "type": "integer"
      },
      "subwayLineName": {
        "type": "keyword"
      },
      "subwayStationName": {
        "type": "keyword"
      },
      "tags": {
        "type": "text"
      },
      "street": {
        "type": "keyword"
      },
      "district": {
        "type": "keyword"
      },
      "description": {
        "type": "text"
      },
      "layoutDesc": {
        "type": "text"
      },
      "traffic": {
        "type": "text"
      },
      "roundService": {
        "type": "text"
      },
      "rentWay": {
        "type": "integer"
      }
    }
  }
}

### 删除xunwu索引
DELETE http://localhost:9200/xunwu

###